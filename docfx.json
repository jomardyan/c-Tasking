{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "build": {
    "content": [
      {
        "files": ["README.md", "docs/**/*.md", "toc.yml"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appName": "c-Tasking",
      "_appTitle": "c-Tasking Documentation",
      "_enableSearch": true
    }
  }
}